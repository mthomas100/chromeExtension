{
  "manifest_version": 2,
  "name": "Google Account Switcher",
  "version": "1.0",
  "description": "Quickly switch between Google accounts.",
  "permissions": ["contextMenus", "tabs"],
  "background": {
    "scripts": ["js/background.js"]
  },
  "browser_action": {
    "default_icon": "images/icon.png"
  },
  "icons": {
    "16": "images/icon.png",
    "32": "images/icon.png",
    "48": "images/icon.png",
    "128": "images/icon.png"
  }
}
